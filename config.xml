<?xml version="1.0" encoding="UTF-8" ?>

<widget id="com.pixelgang.yallseeit" version="3.2" xmlns="http://www.w3.org/ns/widgets" xmlns:android = "http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">

	<name>Yall See It</name>
	<description>Yall see it is a game made for Country Cooking</description>
	<author href="http://www.pixelgangstudios.com" email="support@pixelgangstudios.com">Pixel Gang Studios</author>

	<icon src="www/icon-256.png" width="256" height="256" density="xxxhdpi" />
	<icon src="www/icon-128.png" width="128" height="128" density="xhdpi" />

	<preference name="android-targetSdkVersion" value="26" />

	<preference name="orientation" value="portrait" />
	<preference name="fullscreen" value="true" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="AllowInlineMediaPlayback" value="true" />


	<preference name="deployment-target" value="10.0" />
	<preference name="android-minSdkVersion" value="23" />
	<access origin="*" />

	
<plugin name="cordova-plugin-ad-admob" />
	<plugin name="cordova-plugin-inappbrowser" />
	<plugin name="cordova-plugin-whitelist" />


	
	<engine name="android" />
</widget>
